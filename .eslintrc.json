{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-unresolved": 0,
        "class-methods-use-this": 0,
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "ts": "never",
                "js": "never",
                "": "never"
            }
        ]
    }
}